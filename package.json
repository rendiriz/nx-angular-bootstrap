{
  "name": "nx-angular-bootstrap",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "*** Config ***": "",
    "config:demo": "env-cmd -f ./apps/demo/.env node scripts/demo/env.js",
    "config:admin": "env-cmd -f ./apps/admin/.env node scripts/admin/env.js",
    "*** Development ***": "",
    "dev:demo": "npm run config:demo && nx serve demo --configuration=development --port=4200 --extra-webpack-config=apps/demo/webpack.config.js",
    "dev:admin": "npm run config:admin && nx serve admin --configuration=development --port=4201 --extra-webpack-config=apps/admin/webpack.config.js",
    "*** Build ***": "",
    "build:stag:demo": "npm run config:demo && nx build demo --configuration=staging",
    "build:prod:demo": "npm run config:demo && nx build demo --configuration=production",
    "build:stat:demo": "nx build demo --stats-json",
    "build:stag:admin": "npm run config:admin && nx build admin --configuration=staging",
    "build:prod:admin": "npm run config:admin && nx build admin --configuration=production",
    "build:stat:admin": "nx build admin --stats-json",
    "*** Analyze ***": "",
    "analyze:demo": "npm run build:stat:demo && webpack-bundle-analyzer dist/apps/demo/stats.json",
    "analyze:admin": "npm run build:stat:admin && webpack-bundle-analyzer dist/apps/admin/stats.json",
    "*** Other ***": "",
    "prepare": "husky install",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "postinstall": "ngcc --properties es2015 browser module main"
  },
  "private": true,
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@angular/localize": "^13.3.6",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@nrwl/angular": "^14.1.4",
    "@nrwl/cli": "14.1.4",
    "@nrwl/cypress": "14.1.4",
    "@nrwl/eslint-plugin-nx": "14.1.4",
    "@nrwl/jest": "14.1.4",
    "@nrwl/linter": "14.1.4",
    "@nrwl/workspace": "14.1.4",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "autoprefixer": "10.4.5",
    "cypress": "^9.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "lint-staged": "^12.4.1",
    "lodash-webpack-plugin": "^0.11.6",
    "moment-locales-webpack-plugin": "^1.2.0",
    "ng-packagr": "^13.0.0",
    "ngx-build-plus": "^13.0.1",
    "nx": "14.1.4",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.6.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "~4.6.2",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@ng-bootstrap/ng-bootstrap": "^12.1.1",
    "@ng-select/ng-select": "^8.1.1",
    "@ngx-loading-bar/router": "^6.0.2",
    "@popperjs/core": "^2.11.5",
    "@sweetalert2/ngx-sweetalert2": "^11.0.0",
    "bootstrap": "^5.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "ngx-daterangepicker-material": "^5.0.2",
    "ngx-spinner": "^13.1.1",
    "rendikit-avatar": "^0.0.2",
    "rxjs": "~7.4.0",
    "sweetalert2": "^11.4.14",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "lint-staged": {
    "**/*.{js,ts,md,scss,json,yml}": [
      "prettier --write"
    ],
    "apps/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "libs/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "apps/**/*.{css,scss}": [
      "stylelint --fix"
    ],
    "libs/**/*.{css,scss}": [
      "stylelint --fix"
    ]
  }
}
